None
print ("Здравствуйте  , это бот - объяснялка.           ")
meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "РОФЛ": "шутка",
             "ЩИЩ":  "одобрение или восторг",
             "КРИПОВЫЙ": "страшный, пугающий",
             "АГРИТЬСЯ" : "злиться"
}

for i in range(10):
    word = input("Введите непонятное слово (большими буквами!) Например-ЛОЛ: , затем нажмите ENTER  ")
    if word in meme_dict.keys():
        # Что делать, если слово нашлось?
        print(meme_dict[word])
    else:
        # Что делать, если слово не нашлось?
        print ("cлово не найдено")
